<body>
	<div class="container">
		<div class="jumbotron">
			<h1>Symptom Tracker</h1>
			<h2>How are you feeling today?</h2>
		</div>
	</div>

	<div class="container1">
		<div class="card text-center">
			<div class="card-header">
				<h4 class="card-title">Patient Sign In</h4>
			</div>
			<div class="card-body">
				<form id="userLogin">

					<div class="form-group">
						<label for="userName">Patient Username</label>
						<input type="ID" class="input-block-level" id="userName" aria-describedby="ID Help" placeholder="Enter Username">
						<small id="IDHelp" class="form-text text-muted">We'll never share your ID with anyone else.</small>
					</div>

					<div class="form-group">
						<label for="userPassword">Password</label>
						<input type="password" class="input-block-level" id="userPassword" placeholder="Password">
					</div>

					<input class="btn btn-primary" type="submit" value="Sign In" />

					<a href="/userSignup">Create New Account</a>
				</form>
			</div>
			<!--Copyright-->
			<div class="card-footer">
				Â© 2017 Copyright: <a href="https://github.com/CoreyStagner/SymptomTracker"> Project 2 </a>
			</div>
		</div>
</body>